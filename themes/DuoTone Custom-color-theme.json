{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"focusBorder": "#1f1f31",
		"foreground": "#8885e0",
    "editor.background": "#1b1b2d",
		"editor.foreground": "#8885e0",
		"widget.shadow": "#1f1f31",
		"selection.background": "#1f1f31",
		"textBlockQuote.background": "#1f1f31",
		"textBlockQuote.border": "#262639",
		"textCodeBlock.background":"#7c7b6e",
		"textPreformat.foreground": "#7c7b6e",
		"textLink.foreground": "#fff352",
		"textLink.activeForeground": "#938d2f",
		"dropdown.background": "#1f1f31",
		"dropdown.border": "#262639",
		"dropdown.foreground": "#8885e0",
		"input.background": "#1f1f31",
		"input.border": "#262639",
		"input.foreground": "#8885e0",
		"scrollbarSlider.background": "#232235",
		"scrollbarSlider.activeBackground": "#262639",
		"list.inactiveFocusBackground": "#262639",
		"list.activeSelectionBackground": "#fff352",
		"list.focusBackground": "#232235",
		"activityBar.background": "#1b1b2d",
		"activityBar.foreground": "#8885e0",
		"sideBar.background": "#1b1b2d",
		"sideBar.foreground": "#8885e0",
		"sideBarSectionHeader.background": "#1f1f31",
		"editorGroupHeader.tabsBackground": "#1f1f31",
		"tab.inactiveBackground": "#232235",
		"editor.lineHighlightBackground": "#262639",
		"statusBar.background": "#938d2f",
		"statusBar.foreground": "#c2c1e6",
		"titleBar.activeBackground": "#1f1f31",
		"titleBar.activeForeground": "#8885e0",
		"menu.background": "#1b1b2d",
		"menu.foreground": "#8885e0",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#4a495a",
				"fontStyle": " italic"
			}
		},
		{
			"name": "Comment in Params",
			"scope": "meta.parameters comment.block",
			"settings": {
				"foreground": "#c9c154",
				"fontStyle": " italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#938d2f"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other, variable.other.constant",
			"settings": {
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Variable",
			"scope": "variable, support.other.variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#8885e0"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#7c7b6e"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#7c7b6e"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#c2c1e6"
			}
		},
		{
			"name": "Type Name",
			"scope": "entity.name.type,support.type",
			"settings": {
				"fontStyle": "",
				"foreground": "#c2c1e6"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#c2c1e6"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#c2c1e6"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3f3c96"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag,entity.other.attribute-name.class,entity.other.attribute-name.id",
			"settings": {
				"fontStyle": "",
				"foreground": "#c2c1e6"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#8885e0"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#c2c1e6"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Library class/type",
			"scope": "support.class",
			"settings": {
				"foreground": "#8885e0"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation Meta",
			"scope": "meta.brace,punctuation",
			"settings": {
				"foreground": "#4a495a"
			}
		},
		{
			"name": "C Comment",
			"scope": "comment.block.c",
			"settings": {
				"fontStyle": " italic",
				"foreground": "#c9c154"
			}
		},
		{
			"name": "CSS Property",
			"scope": "meta.property-name,support.type.property-name",
			"settings": {
				"foreground": "#3f3c96"
			}
		},
		{
			"name": "CSS Meta",
			"scope": "meta.property-value",
			"settings": {
				"foreground": "#4a495a"
			}
		},
		{
			"name": "Object Literal Key",
			"scope": "meta.object-literal.key",
			"settings": {
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Import Block Meta",
			"scope": "variable.other.readwrite.alias",
			"settings": {
				"foreground": "#938d2f"
			}
		},
		{
			"name": "Special Objects",
			"scope": "variable.language.this,support.variable.object,support.variable.dom",
			"settings": {
				"foreground": "#3f3c96"
			}
		}
	]
}
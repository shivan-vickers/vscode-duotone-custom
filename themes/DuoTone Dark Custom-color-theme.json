{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"focusBorder": "#1f2431",
		"foreground": "#859fe0",
    "editor.background": "#1b202d",
		"editor.foreground": "#859fe0",
		"widget.shadow": "#1f2431",
		"selection.background": "#1f2431",
		"textBlockQuote.background": "#1f2431",
		"textBlockQuote.border": "#262b39",
		"textCodeBlock.background":"#7c6e72",
		"textPreformat.foreground": "#7c6e72",
		"textLink.foreground": "#ff5283",
		"textLink.activeForeground": "#932f4b",
		"dropdown.background": "#1f2431",
		"dropdown.border": "#262b39",
		"dropdown.foreground": "#859fe0",
		"input.background": "#1f2431",
		"input.border": "#262b39",
		"input.foreground": "#859fe0",
		"scrollbarSlider.background": "#232735",
		"scrollbarSlider.activeBackground": "#262b39",
		"list.inactiveFocusBackground": "#262b39",
		"list.activeSelectionBackground": "#ff5283",
		"list.focusBackground": "#232735",
		"activityBar.background": "#1b202d",
		"activityBar.foreground": "#859fe0",
		"sideBar.background": "#1b202d",
		"sideBar.foreground": "#859fe0",
		"sideBarSectionHeader.background": "#1f2431",
		"editorGroupHeader.tabsBackground": "#1f2431",
		"tab.inactiveBackground": "#232735",
		"editor.lineHighlightBackground": "#262b39",
		"statusBar.background": "#932f4b",
		"statusBar.foreground": "#c1cce6",
		"titleBar.activeBackground": "#1f2431",
		"titleBar.activeForeground": "#859fe0",
		"menu.background": "#1b202d",
		"menu.foreground": "#859fe0",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#494e5a",
				"fontStyle": " italic"
			}
		},
		{
			"name": "Comment in Params",
			"scope": "meta.parameters comment.block",
			"settings": {
				"foreground": "#c95476",
				"fontStyle": " italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#932f4b"
			}
		},
		{
			"name": "User-defined constant",
			"scope": "constant.character, constant.other, variable.other.constant",
			"settings": {
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Variable",
			"scope": "variable, support.other.variable",
			"settings": {
				"fontStyle": "",
				"foreground": "#859fe0"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#7c6e72"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#7c6e72"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#c1cce6"
			}
		},
		{
			"name": "Type Name",
			"scope": "entity.name.type,support.type",
			"settings": {
				"fontStyle": "",
				"foreground": "#c1cce6"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#c1cce6"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#c1cce6"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3c5596"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag,entity.other.attribute-name.class,entity.other.attribute-name.id",
			"settings": {
				"fontStyle": "",
				"foreground": "#c1cce6"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#859fe0"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#c1cce6"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Library class/type",
			"scope": "support.class",
			"settings": {
				"foreground": "#859fe0"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation Meta",
			"scope": "meta.brace,punctuation",
			"settings": {
				"foreground": "#494e5a"
			}
		},
		{
			"name": "C Comment",
			"scope": "comment.block.c",
			"settings": {
				"fontStyle": " italic",
				"foreground": "#c95476"
			}
		},
		{
			"name": "CSS Property",
			"scope": "meta.property-name,support.type.property-name",
			"settings": {
				"foreground": "#3c5596"
			}
		},
		{
			"name": "CSS Meta",
			"scope": "meta.property-value",
			"settings": {
				"foreground": "#494e5a"
			}
		},
		{
			"name": "Object Literal Key",
			"scope": "meta.object-literal.key",
			"settings": {
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Import Block Meta",
			"scope": "variable.other.readwrite.alias",
			"settings": {
				"foreground": "#932f4b"
			}
		},
		{
			"name": "Special Objects",
			"scope": "variable.language.this,support.variable.object,support.variable.dom",
			"settings": {
				"foreground": "#3c5596"
			}
		}
	]
}